/*pattern 27
pyramid  
int n;
cin>>n;
int i=1;
while(i<=n){
//	1st triangle
int space=n-i;
while(space){
	cout<<" ";
	space--;
}

//2nd triangle
int j=1;
while(j<=i){
	cout<<j;
	j++;
}

//3rd triangle
int start = i-1;
while(start){
	cout<<start;
	start--;
}
cout<<endl;
i++;
}

*/

int n;
cin>>n;
int i=1;
while(i<=n){
//	1st trianle
	int no=n-i+1;
	int value=1;
	while(no){
		cout<<value;
		value++;
		no--;
	}
	int j=1;
	while(j<=i){
		cout<<"*";
		j++;
	}
	
	cout<<endl;
	i++;
}
