#include <iostream>
#include <math.h>
using namespace std;
int PowerOfTwo(int n){
//	return true if no is in power of 2 , false if not
	
	int ans=1;
	for(int i=0;i<=30;i++){
		
		if(ans==n){
			return true;
		}
		
		ans = ans*2;
	}
	
	return false;
}

int main(){

int n;
cin>>n;
//	cout<<reverse(n);
cout<<PowerOfTwo(n);
	return 0; 
}
